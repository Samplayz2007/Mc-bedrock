{
  "format_version": "1.10.0",
  "animation_controllers": {
    "controller.animation.sf_nba.firefly.hide": {
      "initial_state": "default",
      "states": {
        "default": {
          "transitions": [
            {
              "hide": "q.property('sf_nba:on_hide')"
            },
            {
              "perish": "q.out_of_control"
            }
          ]
        },
        "hide": {
          "particle_effects": [
            {
              "effect": "hide"
            }
          ]
        },
        "perish": {
          "particle_effects": [
            {
              "effect": "perish"
            }
          ]
        }
      }
    }
  }
}