{
  "format_version": "1.8.0",
  "animations": {
    "animation.sf_nba.look_at_target": {
      "loop": true,
      "bones": {
        "head": {
          "rotation": [
            "q.target_x_rotation",
            "q.target_y_rotation",
            0.0
          ]
        }
      }
    },
    "animation.sf_nba.tortoise.look_at_target": {
      "loop": true,
      "bones": {
        "head": {
          "relative_to": {
            "rotation": "entity"
          },
          "rotation": [
            "math.clamp(q.target_x_rotation, -30, 30)",
            "math.clamp(q.target_y_rotation, -30, 30)",
            0.0
          ]
        }
      }
    }
  }
}