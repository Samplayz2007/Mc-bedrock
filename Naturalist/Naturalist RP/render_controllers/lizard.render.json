{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.sf_nba.lizard": {
      "arrays": {
        "textures": {
          "array.skins": [
            "texture.beardie",
            "texture.brown",
            "texture.green",
            "texture.leopard_gecko"
          ]
        }
      },
      "geometry": "geometry.default",
      "materials": [
        {
          "*": "material.default"
        }
      ],
      "textures": [
        "array.skins[q.variant]"
      ],
      "part_visibility": [
        {
          "sleep*": "q.is_sleeping"
        },
        {
          "tail*": "q.property('sf_nba:has_tail')"
        }
      ]
    }
  }
}