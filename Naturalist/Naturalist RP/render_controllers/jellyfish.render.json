{
  "format_version": "1.20.0",
  "render_controllers": {
    "controller.render.sf_nba.jellyfish": {
      "geometry": "geometry.default",
      "materials": [
        {
          "*": "material.default"
        },
        {
          "*inflation*": "material.inflation"
        }
      ],
      "arrays": {
        "textures": {
          "Array.skins": [
            "Texture.white",
            "Texture.yellow",
            "Texture.pink",
            "Texture.blue",
            "Texture.green"
          ]
        }
      },
      "textures": [
        "Array.skins[q.property('sf_nba:variant')]"
      ]
    }
  }
}