{
  "format_version": "1.8.0",
  "minecraft:spawn_rules": {
    "description": {
      "identifier": "sf_nba:owl",
      "population_control": "animal"
    },
    "conditions": [
      {
        "minecraft:spawns_on_surface": {},
        "minecraft:brightness_filter": {
          "min": 7,
          "max": 15,
          "adjust_for_weather": false
        },
        "minecraft:weight": {
          "default": 6
        },
        "minecraft:herd": {
          "min_size": 1,
          "max_size": 1
        },
        "minecraft:biome_filter": {
          "any_of": [
            {
              "test": "has_biome_tag",
              "operator": "==",
              "value": "taiga"
            },
            {
              "test": "has_biome_tag",
              "operator": "==",
              "value": "forest"
            },
            {
              "test": "has_biome_tag",
              "operator": "==",
              "value": "snowy_slopes"
            }
          ]
        }
      }
    ]
  }
}