            {
              "test": "has_biome_tag",
              "operator": "==",
              "value": "flower_forest"
            },
            {
              "test": "has_biome_tag",
              "operator": "==",
              "value": "plains"
            },
            {
              "test": "has_biome_tag",
              "operator": "==",
              "value": "swamp"
            },
            {
              "test": "has_biome_tag",
              "operator": "==",
              "value": "mangrove_swamp"
            }
          ]
        }
      }
    ]
  }
}